{
    "pipeline": {
      "dev": {
        "dependsOn": [
          "^db:generate"
        ],
        "cache": false
      },
      "type-check": {
        "outputs": []
      },
      "format": {
        "cache": false
      },
      "build": {
        "cache": false,
        "dependsOn": [
          "^db:generate",
          "^build"
        ],
        "outputs": [
          "dist/**"
        ]
      },
      "db:generate": {
        "cache": false
      },
      "db:push": {
        "cache": false
      },
      "db:deploy": {
        "cache": false
      }
    }
  }